    @model List<VeyselAlanWebPage.Models.PortfolioItem>

<section id="projects">
    @foreach (var item in Model)
    {
        <div class="project-card">
            <img src="@item.ImageUrl" alt="Proje 1">
            <h3>@item.ProjectName</h3>
            <p>@item.Description</p>
            <a href="">Detayları Gör</a>
        </div>
    }

 
</section>

